{
  "check-coverage": true,
  "per-file": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 50,
  "require": [
    "babel-register",
    "regenerator-runtime/runtime"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/UI/**",
    ".nyc_output",
    "**/server/controller**",
    "**/server/middleware**",
    "**/server/db**"
  ],
  "include": [
  "**server/routes**",
  "**server/controller/utils**",
  "**server/config**"
  ],
  "reporter": ["lcov", "json", "text", "html"],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "report-dir": "./coverage"
}