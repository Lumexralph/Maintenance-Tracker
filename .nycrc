{
  "check-coverage": true,
  "per-file": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 50,
  "require": [
    "babel-register",
    "regenerator-runtime/runtime"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/tests2/**",
    "**/UI/**",
    ".nyc_output",
    "**/server/src/controller**",
    "**/server/src/controller2**",
    "**/server/src/datastore**",
    "**/server/src/model**",
    "**/server/src/middleware**",
    "**/server/src/api**"
  ],
  "include": [
    
  ],
  "reporter": ["lcov", "json", "text", "html"],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "report-dir": "./coverage"
}